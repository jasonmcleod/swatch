pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
playing = true
skip = false
level = 1
x = 0
grid_h = 15
grid_w = 15
grid = {}
slide = 0
blank = 13
maxheight = 10
ticks = 0
lastplacement = 0
placerate = 5
streak = 0
score = 0
function buildband()
	band = {}
	for c = 0, 15 do
		band[c] = flr(rnd(3))
	end
	return band
end
band1 = buildband()
band2 = buildband()

function draw_menu()
 cls()
 draw_intro_effects()
 sspr(0, 32, 128, 69,0,1)
 
 print("play swatch",40,50,7)
 print("instructions", 40,60,7)
 print("a game by jason mcleod",20,100,8)
 print("for '1 day jam' 2016",24,110,9)
 print("@jasonmcleod / jasonmcleod.me",5,120,10)
end

fx_x = 0
function draw_intro_effects()
	local offset_y = 5
	for y = 0, 4 do
	 for x = -4, 26 do
   spr(48,x*8+fx_x-1,y*8)
	 end
	end
	fx_x += 1
	if fx_x > 23 then fx_x = 0 end
end


-- fill level
function buildlevel()
	for y = 0, grid_h do
	 grid[y] = {}
	 for x = 0, grid_w do
	  u = flr(rnd(3)+3)
 	 if y >= level then u = blank end
	  grid[y][x] = u
	 end
	end
end

function swatch() 
 z = flr(x/8)+1

 if band1[z] == 0 and band2[z] == 0 then return 0 end -- red + red = red
	if band1[z] == 0 and band2[z] == 1 then return 3 end -- red + blue  = purple
	if band1[z] == 0 and band2[z] == 2 then return 4 end -- red + yellow = orange

	if band1[z] == 1 and band2[z] == 0 then return 3 end -- blue + blue = blue
	if band1[z] == 1 and band2[z] == 1 then return 1 end -- blue + red = purple
	if band1[z] == 1 and band2[z] == 2 then return 5 end -- blue + yellow = green

	if band1[z] == 2 and band2[z] == 0 then return 4 end -- yellow + red = orange
	if band1[z] == 2 and band2[z] == 1 then return 5 end -- yellow + blue = green
	if band1[z] == 2 and band2[z] == 2 then return 2 end -- yellow + yellow = yellow
 return 1
end

function nudgeright(tab)
local x, v, temp
 temp = {tab[17]}
 for x,v in pairs(tab) do
  local c = x+1
  if c > 17 then c = 0 end
  temp[c] = v
 end
 return temp
end

function nudgeleft(tab)
	local x, v, temp
 temp = {}
 for x,v in pairs(tab) do
  local c = x-1
  if c <= 0 then c = 17 end
  temp[c] = v
 end
 temp[17] = tab[1]
 return temp
end

function check(height)
  local z = flr(x/8)
  if grid[height-1][z] == grid[height][z] then
   grid[height-1][z] = blank
   grid[height][z] = blank
   return true
  else
   return false
  end	
end

function getheight(xx)
 for y = 1, grid_h do
  if grid[y][xx] == blank then return y end
	end
	return level
end

function gameover()
 playing = false
end

function place()
 -- limit placement speed
 if ticks - lastplacement < placerate then return end

 -- check if we are topped out
 for a = 1, grid_w do
  if getheight(a) >= maxheight then
   gameover()
  end
	end
	
 local height = getheight(x/8)
 grid[height][x/8] = swatch()
 if check(height) then
	 streak+=1
	 score+=10 * streak
  sfx(2)
 else
 	streak=0
 	sfx(4)
 end
-- sfx(1)
 lastplacement = ticks
end

function draw_header() 
	spr(32,0,0) -- s
	spr(33,6,0) -- w
	spr(34,14,0)-- a
	spr(35,20,0)-- t
	spr(36,26,0)-- c
	spr(37,32,0)-- h
	
	print("score: " .. score,0,10,7)
	print("streak: " .. streak, 90,10,7)
end

function _update()
 ticks+=1
 if skip then return end
 if btn(0) then x-=8 end
 if x < 0 then x = 15*8 end
 if btn(1) then x += 8 end
 if x > 15*8 then x = 0 end
 if btn(2) then
  place()
 end
end

function _draw()
 if playing then
	 cls()
  local p = swatch()
 	slide += .25
 	
 	
 	
 	
  -- allow for flickering
 	skip = not skip
 
 	if slide == 8 then
 		band1 = nudgeleft(band1)
 		band2 = nudgeright(band2)
 		slide = 0
 	end
 	
  -- draw player
  spr(p+6, x, 15*8)
   
  -- draw level
  for y = 0, grid_h do
   for x = 0, grid_w do
    local col = grid[y][x]
	    spr(col, x*8, y*8+16)
   end
 	end
 	
 	
 	--draw bands
  for x = 0, 17 do
 	 spr(band1[x], x*8-slide-8, 13*8)
 	 spr(band2[x], x*8+slide-8, 14*8)
  end
  
 	-- draw line from top to bottom
  for y = getheight(x/8), 10 do
  	spr(p+16, x, y*8+16)
 	end
 	
 	line(x-1, 8, x-1, 16*8-2,0)	
 	line(x+7, 8, x+8, 16*8-2,0)	
  	
  draw_header()	
  	
 else 
 	draw_menu()
--  print("game over",0,0,14)
 end
 
end 
buildlevel()
